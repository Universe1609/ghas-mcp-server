{
  "name": "@rajbos/ghas-mpc-server",
  "version": "0.0.3",
  "description": "GitHub Advanced Security MCP server",
  "license": "MIT",
  "type": "module",
  "bin": {
    "ghas-mpc-server": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node -e \"if (process.platform === 'win32') { require('child_process').execSync('tsc'); } else { require('child_process').execSync('tsc && shx chmod +x dist/*.js'); }\"",
    "prepare": "npm run build",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.0.1",
    "@octokit/rest": "^21.1.1",
    "@types/node": "^22",
    "@types/node-fetch": "^2.6.12",
    "node-fetch": "^3.3.2",
    "universal-user-agent": "^7.0.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "shx": "^0.3.4",
    "typescript": "^5.6.2"
  }
}